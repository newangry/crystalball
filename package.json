{
  "name": "ecotopia-today",
  "version": "0.0.1",
  "author": "edwardsharp",
  "description": "ecotopia.today openlayers component prototype",
  "repository": "github:edwardsharp/ecotopia-today",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && ./util/minify_dist_geojson.js",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "mediabox": "^1.1.2",
    "ol": "^5.1.3",
    "simplify-geojson": "^1.0.3",
    "vue": "^2.5.16",
    "vue-gallery": "^1.5.0",
    "vue-image-lightbox": "^6.0.0",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "gh-pages": "^1.2.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
